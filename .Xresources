rofi.color-enabled: true
rofi.font:Fantasque\ Sans\ Mono 11
!                  bg       border   separator
rofi.color-window: #282828, #282828, #282828
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #282828, #ebdbb2, #282828, #282828, #458588
rofi.color-active: #282828, #282828, #282828, #282828, #fabd2f
rofi.color-urgent: #282828, #282828, #282828, #282828, #cc241d

xterm.borderWidth: 0
xterm.termName: xterm-256-color
! -----------------------------------------------------------------------------
URxvt*depth: 				            32
!URxvt*foreground     : #ebdbb2 
URxvt*background   	: [90]#171f27
!URxvt*cursorColor    	: #c5c8c6


URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:past


!rgba:0000/0000/0200/c800 

!! clickable urls
URxvt.perl-ext-common:      default,matcher,selection-to-clipboard,clipboard
URxvt.url-launcher:         /usr/bin/xdg-open
URxvt.matcher.button:       1
URxvt.url-select.underline: true
URxvt.scrollBar: false
URxvt.geometry: 90x30
!! Fonts etc
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!URxvt*font:xft:Monaco:pixelsize=11,style=regular
!URxvt*font:xft:DejaVu Sans Mono:pixelsize=11,style=regular
!URxvt.boldFont:xft:DejaVu Sans Mono:style=bold:pixelsize=11
URxvt.font:             xft:xos4 Terminess Powerline:style=Regular:pixelsize=10
URxvt.boldFont:         xft:xos4 Terminess Powerline:style=Bold:pixelsize=11

urxvt*intensityStyles:        false
urxvt*skipBuiltinGlyphs:    true
urxvt*linespace:            0
#*fading: 15 
xterm*faceName: DejaVu Sans Mono
xterm*faceSize: 8
XTerm.vt100.locale: true




